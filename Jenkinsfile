pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build: Hello World by build'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Test: Hello World by test'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deploy: Hello World by deploy'
            }
        }
    }
}
